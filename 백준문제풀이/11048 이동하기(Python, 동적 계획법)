n, m = map(int, input().split())
dp = []

for i in range(n):
    dp.append([*map(int, input().split())])

for j in range(1, m):
    dp[0][j] += dp[0][j - 1]

for i in range(1, n):
    for j in range(m):
        if j == 0:
            dp[i][j] += dp[i - 1][j]
        else:
            dp[i][j] += max(dp[i - 1][j], max(dp[i - 1][j - 1], dp[i][j - 1]))

print(max( max(dp) ))
