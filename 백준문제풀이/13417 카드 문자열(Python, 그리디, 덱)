for _ in range(int(input())):
    n = int(input())
    cards = input().split()
    result = cards[0]  # 첫 글자 시작
    for i in range(1, n):
        if cards[i] <= result[0]:  # 현재 카드가 result 맨 앞보다 작거나 같으면
            result = cards[i] + result  # 왼쪽에 붙이기
        else:
            result = result + cards[i]  # 오른쪽에 붙이기
    print(result)
