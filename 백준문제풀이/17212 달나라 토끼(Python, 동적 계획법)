n = int(input())
dp = [0, 1, 1, 2, 2, 1, 2, 1] # 0, 1, 2, 3, 4, 5, 6, 7ì›

for i in range(8, n + 1, 1):
    minimum = 0
    if dp[i - 7] != 0:
        minimum = dp[i - 7] + 1
    if dp[i - 5] != 0 and (dp[i - 5] + 1) < minimum:
        minimum = dp[i - 5] + 1
    if dp[i - 2] != 0 and (dp[i - 2] + 1) < minimum:
        minimum = dp[i - 2] + 1
    if dp[i - 1] != 0 and (dp[i - 1] + 1) < minimum:
        minimum = dp[i - 1] + 1
    dp.append(minimum)

print(dp[n])
