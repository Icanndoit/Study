#include <iostream>
using namespace std;

int N, M;
int arr[8];        // 현재 수열
bool used[9];      // 숫자 사용 여부 체크

void DFS(int depth) {
    if (depth == M) {          // 수열 길이가 M이면 출력
        for (int i = 0; i < M; i++) 
            cout << arr[i] << " ";
        cout << "\n";
        return;
    }
    for (int i = 1; i <= N; i++) {  // 1~N 시도
        if (!used[i]) {             // 사용되지 않은 숫자라면
            used[i] = true;
            arr[depth] = i;
            DFS(depth + 1);         // 다음 자리로 이동
            used[i] = false;        // 백트래킹
        }
    }
}

int main() {
    cin >> N >> M;
    DFS(0);
}
