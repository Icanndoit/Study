n = int(input())
num = []

for _ in range(n):
    num.append(input())

cnt = 3  # 뒤에서 3자리부터 시작
while True:
    seen = []
    duplicate = False

    for s in num:
        part = s[-cnt:]
        if part in seen:
            duplicate = True
            break
        seen.append(part)
    
    if not duplicate:
        print(cnt)
        break
    
    cnt += 1
