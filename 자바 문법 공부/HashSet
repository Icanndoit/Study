import java.util.HashSet;
// 순서와 상관없이 중복을 허용하지 않는 경우에는 set 인터페이스를 구현한 클래스를 사용한다.
// 중복을 허용하지 않는 것 ex.) 주민등록번호, 학번, 기타 등등
// set 인터페이스를 구현한 클래스 예시 : HashSet, TreeSet

public class Main {

    public static void main(String[] args)
    {
        HashSet<String> set = new HashSet<String>();

        System.out.println("HashSet 크기 : " + set.size());
        System.out.println("비어있는가? " + set.isEmpty());
        System.out.println(set);
        System.out.println();

        set.add(new String("Lebron"));
        set.add(new String("Durant"));
        set.add(new String("Curry"));
        set.add(new String("Harden"));

        System.out.println(set);
        System.out.println("HashSet 크기 : " + set.size());
        System.out.println("비어있는가? " + set.isEmpty());
        System.out.println("Is contains Harden? "+ set.contains("Harden"));
        set.remove("Harden");
        System.out.println("Is contains Harden? "+ set.contains("Harden"));
        System.out.println(set);

        // set.add(new String("Curry"));  이미 존재하므로 추가되지 않는다. 즉 중복 불가!!
        
    }
}
