package Practice;
import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        PrivateTest con = new PrivateTest();

        con.setage(40);  // setage 메서드를 활용해 private 변수에 접근 가능
