class Person {
    String name;
    int age;

    Person() {
        this("이름 없음", 1);
    }
    Person(String name, int age) {
        this.name = name;
        this.age = age;
    }

    Person returnadd() {  // 반환명은 클래스명
        return this;
    }
}

public class Main {

    public static void main(String[] args)
    {
        
        Person noName = new Person();
        System.out.println(noName.name);
        System.out.println(noName.age);

        Person p = noName.returnadd();  // this 값을 클래스 변수에 대입
        System.out.println(p);  // noName.returnadd() 의 반환값 출력
        System.out.println(noName);   // 참조 변수 출력

    }
}
